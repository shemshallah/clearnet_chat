# Environment Variables for Clearnet Chat
# Copy this to .env for local development

# Secret key for JWT tokens (Render will auto-generate in production)
SECRET_KEY=your-secret-key-here-change-in-production

# Database URL (Render will auto-populate from database connection)
DATABASE_URL=postgresql://user:password@host:5432/dbname

# Allowed origins for CORS (comma-separated)
ALLOWED_ORIGINS=https://clearnet-gate.onrender.com,http://localhost:3000

# Port (Render will set this automatically)
PORT=8000
